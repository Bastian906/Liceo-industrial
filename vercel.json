{
  "version": 2,
  "functions": {
    "api/index.js": { "runtime": "nodejs18.x" }
  },
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/index.js" },

    { "src": "^/admin$", "dest": "/frontend/admin.html" },
    { "src": "^/$", "dest": "/frontend/index.html" },

    { "src": "^(?!/api)(.*)$", "dest": "/frontend$1" }
  ]
}
